{
  "name": "@formio/semantic",
  "version": "1.2.4",
  "description": "Semantic UI templates for form.io forms.",
  "main": "lib/index.js",
  "scripts": {
    "test:coverage": "nyc --reporter=text mocha --reporter spec './{,!(node_modules)/**/}*.spec.js'",
    "test": "mocha --require ts-node/register --reporter spec './{,!(node_modules)/**/}*.spec.ts'",
    "watch": "tsc -w",
    "build": "tsc && gulp templates && webpack",
    "lint": "tslint -p .",
    "prepublish": "npm run build"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "keywords": [
    "Semantic",
    "UI",
    "form.io",
    "forms"
  ],
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/ejs": "^2.6.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.11",
    "@types/sinon": "^7.5.1",
    "chai": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^1.4.0",
    "gulp-template": "^5.0.0",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2",
    "sinon": "^7.5.0",
    "ts-node": "^8.5.2",
    "ts-sinon": "^1.0.24",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formio/semantic.git"
  },
  "author": "randall@form.io",
  "license": "ISC"
}
